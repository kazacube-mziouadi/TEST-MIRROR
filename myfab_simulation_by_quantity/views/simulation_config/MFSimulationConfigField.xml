<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record id='mf_simulation_config_field' model="ir.actions.act_window">
        	<field name="name">mf_simulation_config_field</field>
			<field name="res_model">mf.simulation.config.field</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
        </record>

		<record id='mf_simulation_config_field_tree' model="ir.ui.view">
        	<field name="name">mf_simulation_config_field_tree</field>
			<field name="model">mf.simulation.config.field</field>
            <field name="arch" type="xml">
				<tree string="myfab Simulation by quantity field's configs" editable="bottom" create="0" delete="0">
					<field name="sequence" readonly="1" invisible="1"/>
					<field name="mf_field_id" readonly="1"/>
					<field name="mf_is_visible"/>
				</tree>
			</field>
        </record>

        <record id='mf_simulation_config_field_form' model="ir.ui.view">
        	<field name="name">mf_simulation_config_field_form</field>
			<field name="model">mf.simulation.config.field</field>
            <field name="arch" type="xml">
				<form string="myfab Simulation by quantity field's config" version="8.0">
					<group col="1">
						<field name="sequence" readonly="1" invisible="1"/>
						<field name="mf_field_id" readonly="1"/>
						<field name="mf_is_visible"/>
					</group>
				</form>
			</field>
		</record>
    </data>
</openerp>