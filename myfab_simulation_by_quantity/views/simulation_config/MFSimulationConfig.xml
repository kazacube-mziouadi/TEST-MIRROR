<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record id='mf_simulation_config' model="ir.actions.act_window">
        	<field name="name">myfab Simulation configuration</field>
			<field name="res_model">mf.simulation.config</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="context">{'create':False, 'copy': False, 'delete': False, 'edit':True}</field>
        </record>

		<record id='mf_simulation_config_tree' model="ir.ui.view">
        	<field name="name">mf_simulation_config_tree</field>
			<field name="model">mf.simulation.config</field>
            <field name="arch" type="xml">
				<tree string="myfab Simulation by quantity configs">
					<field name="name"/>
				</tree>
			</field>
        </record>

        <record id='mf_simulation_config_form' model="ir.ui.view">
        	<field name="name">mf_simulation_config_form</field>
			<field name="model">mf.simulation.config</field>
            <field name="arch" type="xml">
				<form string="myfab Simulation by quantity config">
					<group col="1">
						<field name="mf_fields_ids" />
					</group>
				</form>
			</field>
		</record>
    </data>
</openerp>