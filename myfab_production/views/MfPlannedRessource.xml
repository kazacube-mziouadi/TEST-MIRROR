<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record id='mf_planned_ressource' model="ir.actions.act_window">
        	<field name="name">myfab planned ressource</field>
			<field name="res_model">mf.planned.ressource</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
        </record>

		<record id='mf_planned_ressource_treeview' model="ir.ui.view">
        	<field name="name">myfab planned ressource</field>
			<field name="model">mf.planned.ressource</field>
            <field name="arch" type="xml">
				<tree string="myfab planned ressource">
					<field name="name"/>
					<field name="mf_description"/>
					<field name="mf_number_ressources"/>
				</tree>
			</field>
        </record>

        <record id='mf_planned_ressource_form' model="ir.ui.view">
        	<field name="name">mf.planned.ressource.form</field>
			<field name="model">mf.planned.ressource</field>
            <field name="arch" type="xml">
				<form string="myfab planned ressource">
					<group>
                        <field name="name"/>
                        <field name="mf_description"/>
                        <field name="mf_number_ressources"/>
                        <field name="mf_color" color="True"/>
					</group>
				</form>
			</field>
		</record>
    </data>
</openerp>